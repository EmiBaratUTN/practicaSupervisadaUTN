	<queryString>
		<![CDATA[select a.nombres, a.apellido, p.fechaPesaje, p.pesoEnKilos, tp.descripcion, p.IMC, p.observaciones
from pesajes p
inner join alumnos a on a.idAlumno = p.idAlumno
inner join tiposEstadosPeso tp on tp.idTipoEstadosPeso = p.idEstadoPeso
where p.idEstadoPeso = $P{idEstadoPeso}
and a.nombres = $P{nombre}]]>
	</queryString>
